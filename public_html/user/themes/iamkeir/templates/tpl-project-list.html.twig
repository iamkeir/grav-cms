{% extends 'layouts/base.html.twig' %}
{% set projects = page.collection({ items: '@self.children' }) %}

{% block content %}

  <h1>{{ page.title }}</h1>

  {{ page.content|raw }}

  <ul>
    {% for proj in projects %}
      <li>
        <a href="{{ proj.url }}">{{ proj.title }}</a>
      </li>
    {% endfor %}
  </ul>

{% endblock %}
